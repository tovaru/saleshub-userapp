{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport AuthDataService from \"../services/AuthDataService\";\nexport default {\n  name: \"add-auth\",\n  data: function data() {\n    return {\n      auth: {\n        email: \"\",\n        password: \"\"\n      },\n      submitted: false\n    };\n  },\n  methods: {\n    tryLogin: function tryLogin() {\n      var _this = this;\n\n      var data = {\n        email: this.auth.email,\n        password: this.auth.password\n      };\n      AuthDataService.login(data).then(function (response) {\n        console.log(response);\n        _this.submitted = true;\n      }).catch(function (e) {\n        console.log(e);\n      });\n    },\n    newAuth: function newAuth() {\n      this.submitted = false;\n      this.auth = {};\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsCA;AACA;EACAA,gBADA;EAEAC,IAFA,kBAEA;IACA;MACAC;QACAC,SADA;QAEAC;MAFA,CADA;MAKAC;IALA;EAOA,CAVA;EAWAC;IACAC,QADA,sBACA;MAAA;;MACA;QACAJ,sBADA;QAEAC;MAFA;MAIAI,4BACAC,IADA,CACA;QACAC;QACA;MACA,CAJA,EAKAC,KALA,CAKA;QACAD;MACA,CAPA;IAQA,CAdA;IAeAE,OAfA,qBAeA;MACA;MACA;IACA;EAlBA;AAXA","names":["name","data","auth","email","password","submitted","methods","tryLogin","AuthDataService","then","console","catch","newAuth"],"sourceRoot":"src/components","sources":["Login.vue"],"sourcesContent":["<template>\n  <div class=\"submit-form mt-3 mx-auto\">\n    <p class=\"headline\">Login</p>\n    <div v-if=\"!submitted\">\n      <v-form ref=\"form\" lazy-validation>\n        <v-text-field\n          v-model=\"auth.email\"\n          :rules=\"[(v) => !!v || 'Email is required']\"\n          label=\"Email\"\n          required\n        ></v-text-field>\n        <v-text-field\n          v-model=\"auth.password\"\n          :rules=\"[(v) => !!v || 'Password is required']\"\n          label=\"Password\"\n          required\n        ></v-text-field>\n      </v-form>\n      <v-btn color=\"primary\" class=\"mt-3\" @click=\"tryLogin \">Submit</v-btn>\n      <v-btn color=\"accent\" elevation=\"2\" to=\"/register\" text>Register</v-btn>\n    </div>\n    <div v-else>\n      <v-card class=\"mx-auto\">\n        <v-card-title>\n          Logged in successfully!\n        </v-card-title>\n        <v-card-subtitle>\n          Click the button to loggin.\n        </v-card-subtitle>\n        <v-card-actions>\n          <v-btn color=\"success\" @click=\"newAuth \">Add</v-btn>\n        </v-card-actions>\n      </v-card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AuthDataService from \"../services/AuthDataService\";\nexport default {\n  name: \"add-auth\",\n  data() {\n    return {\n      auth: {\n        email: \"\",\n        password: \"\",\n      },\n      submitted: false,\n    };\n  },\n  methods: {\n    tryLogin() {\n      var data = {\n        email: this.auth.email,\n        password: this.auth.password,\n      };\n      AuthDataService.login(data)\n        .then((response) => {\n          console.log(response);\n          this.submitted = true;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    newAuth() {\n      this.submitted = false;\n      this.auth = {};\n    },\n  },\n};\n</script>\n<style>\n.submit-form {\n  max-width: 300px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}